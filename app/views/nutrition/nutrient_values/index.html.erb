<p id="notice"><%= notice %></p>

<h1>Listing Nutrient Values</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Nutrient</th>
      <th>Yield</th>
      <th>Is fortified</th>
      <th>Estimated from</th>
      <th>Last update</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nutrient_values.each do |nutrient_value| %>
      <tr>
        <td><%= nutrient_value.food %></td>
        <td><%= nutrient_value.nutrient %></td>
        <td><%= nutrient_value.yield %></td>
        <td><%= nutrient_value.is_fortified %></td>
        <td><%= nutrient_value.estimated_from %></td>
        <td><%= nutrient_value.last_update %></td>
        <td><%= link_to 'Show', nutrient_value %></td>
        <td><%= link_to 'Edit', edit_nutrient_value_path(nutrient_value) %></td>
        <td><%= link_to 'Destroy', nutrient_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nutrient value', new_nutrient_value_path %>
